<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!-- 设置别名
     如果类名上有注解，则使用注解@Alias指定的名称作为别名，
     如果没有则使用类名首字母小写作为别名 -->
    <typeAliases>
        <package name="com.xingdong.entity" />
    </typeAliases>
</configuration>